rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }

    match /users/{user} {
      allow read: if request.auth.uid == user;
      allow delete: if false;
      allow update: if request.resource.data.uid == resource.data.uid;
    }
  }
}